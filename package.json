{
  "name": "Proto",
  "author": "Adrian C. Miranda",
  "description": "An extensible program-code-template for creating objects",
  "version": "1.1.0",
  "types": "./source/index.d.ts",
  "module": "./source/index.js",
  "main": "./dist/Proto.umd.js",
  "homepage": "https://github.com/adriancmiranda/Proto",
  "license": "MIT",
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "source"
  ],
  "scripts": {
    "bin": "rollup -c bin/index.js",
    "analyze:amd": "source-map-explorer dist/type.@.amd.min.js.map",
    "analyze:iife": "source-map-explorer dist/type.@.iife.min.js.map",
    "analyze:umd": "source-map-explorer dist/type.@.umd.min.js.map",
    "report:coverage": "npm run test:cover && codecov -f test/coverage.lcov --token=$CODECOV_TOKEN",
    "build:doc": "documentation build index.js -f html -o docs",
    "build:gzip": "npm run bin -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run bin -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run bin -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build:example": "cd example && node",
    "build": "redrun -p build:concat build:minify build:gzip",
    "test:sauce:modern": "karma start bin/karma/sauce.config.js --env modern",
    "test:sauce:mobile": "karma start bin/karma/sauce.config.js --env mobile",
    "test:sauce:legacy": "karma start bin/karma/sauce.config.js --env legacy",
    "test:sauce": "redrun -s test:sauce:*",
    "sauce": "karma start bin/karma/sauce.config.js",
    "test:travis": "karma start bin/karma/ci.config.js",
    "test:appveyor": "karma start bin/karma/ci.config.js",
    "test:cover": "cross-env NODE_ENV=test karma start bin/karma/cover.config.js",
    "test:unit": "cross-env NODE_ENV=test karma start bin/karma/unit.config.js",
    "test:speed": "node bin/benchmark",
    "test": "redrun -s lint flow test:unit",
    "dev": "cross-env NODE_ENV=test karma start bin/karma/dev.config.js",
    "watch": "npm run bin -- -w --environment=NODE_ENV:development",
    "lint": "eslint --ext .js index.js source",
    "flow": "flow",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "precommit": "npm run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version",
    "untest": "rimraf *.log test/unit/coverage",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "uninstall": "rimraf -rf node_modules package-lock.json"
  },
  "dependencies": {
    "describe-type": "0.4.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "benchmark": "2.1.4",
    "chokidar": "1.7.0",
    "codecov": "3.0.0",
    "conventional-commit-types": "2.2.0",
    "cross-env": "5.1.1",
    "documentation": "5.3.3",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "flow-bin": "0.58.0",
    "fs-extra": "4.0.2",
    "git-revision-webpack-plugin": "2.5.1",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "js-yaml": "3.10.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-failed-reporter": "0.0.3",
    "karma-firefox-launcher": "1.0.1",
    "karma-fixture": "0.2.6",
    "karma-html2js-preprocessor": "1.1.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jasmine-matchers": "3.7.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "1.5.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "1.2.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.5",
    "make-arrow-function": "1.1.0",
    "make-generator-function": "1.1.0",
    "moment": "2.19.1",
    "npm-check-updates": "2.13.0",
    "pirate-flag": "0.1.0",
    "redrun": "5.9.18",
    "remove-flow-types-loader": "1.1.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-es3": "1.0.3",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-gzip": "1.3.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1",
    "sinon": "4.1.1",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.2.0",
    "strip-json-comments": "2.0.1",
    "tosource": "1.0.0",
    "uglify-es": "3.1.6",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/Proto.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "keywords": [
    "es3",
    "class",
    "utility"
  ]
}
